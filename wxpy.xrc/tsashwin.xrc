<?xml version="1.0"?>
<resource>
  <object class="wxFrame" name="TestSashFrame">
    <title>Test of SashLayoutWindow in XRC</title>

        <object class="SashPanel">
          <object class="wxSashLayoutWindow">
            <flag>wxNO_BORDER|wxSW_BORDER</flag>
            <orientation>wxLAYOUT_HORIZONTAL</orientation>
            <alignment>wxLAYOUT_TOP</alignment>
            <sashvisible>wxSASH_BOTTOM</sashvisible>
            <background>#FFFF00</background>
            <defaultsize>320,36</defaultsize>
            <extrabordersize>4</extrabordersize>
            <object class="wxStaticText">
              <label>First</label>
            </object>
          </object>
          <object class="wxSashLayoutWindow">
            <flag>wxNO_BORDER|wxSW_BORDER</flag>
            <orientation>wxLAYOUT_HORIZONTAL</orientation>
            <alignment>wxLAYOUT_TOP</alignment>
            <sashvisible>wxSASH_BOTTOM</sashvisible>
            <background>#00FFFF</background>
            <defaultsize>320,36</defaultsize>
            <extrabordersize>4</extrabordersize>
            <object class="wxStaticText">
              <label>second</label>
            </object>
          </object>
          <object class="wxPanel">
            <object class="wxStaticText">
              <label>third</label>
            </object>
          </object>
        </object>

  </object>
</resource>
